
=. WebServer
 | Sewer.latest
   <| File.watch @./index.html
      -. Sewer File
   <| Socket.listen @*:80
      -. Sewer (Socket Byte)
      =| Http.Request.fromBytes
      -. Sewer (Socket Http)
 . ( File, Socket Http.Request )
 | @. ( File, Http.Request ) -> Http.Response
      ( File index, Request { #endpoint @/ } ) -> index
      @| MyDB.getRandomUserInfo
       * Html.Template.render index
       : Http.Error.ServerError
    : Http.Error.NotFound
 . Http.Response Html
